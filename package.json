    "axios": "^0.19.2",
    "better-sqlite3": "^7.1.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "draftlog": "^1.0.12",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "lowdb": "^1.0.0",
    "marked": "^1.1.0",
    "node-fetch": "^2.6.0",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.2.0",
    "threads": "^1.4.1",
    "time-stamp": "^2.2.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.2.1",
    "@antv/g2plot": "^1.1.7",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.9.0",
